v_a = [26.027217864990234, 3.004024028778076, 104.83576965332031, 0.10460533201694489, 82.89033508300781, 0.25589773058891296, 3.6291615962982178, 3.613302230834961, 1.8612794876098633, 0.10220879316329956]
